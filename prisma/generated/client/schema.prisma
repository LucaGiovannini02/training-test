generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TRichieste {
  RichiestaID              Int      @id @unique @default(autoincrement())
  CognomeNomeRichiedente   String   @unique
  DataInserimentoRichiesta DateTime @default(now())
  Importo                  Int
  NumeroRate               Int
}
